  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.9.0",
    "babel-core": "^6.7.6",
    "babel-preset-es2015": "^6.6.0",
    "babel-register": "^6.7.2",
    "babelify": "^7.2.0",
    "browserify": "^11.2.0",
    "deglobalify": "^0.2.0",
    "es6ify": "^1.6.0",
    "expect.js": "^0.3.1",
    "gulp": "^3.9.0",
    "gulp-connect": "^2.2.0",
    "gulp-jshint": "^1.11.2",
    "gulp-uglify": "^1.4.1",
